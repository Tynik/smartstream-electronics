{
  "name": "smartstream-electronics",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "sitemap": "tsc -p tsconfig.generators.json && tsc-alias -p tsconfig.generators.json && node generators/dist/generators/sitemap-generator.js",
    "products": "tsc -p tsconfig.generators.json && tsc-alias -p tsconfig.generators.json && node generators/dist/generators/products-generator.js",
    "build": "NODE_ENV=production webpack",
    "deploy": "yarn run build",
    "start:netlify": "NODE_ENV=development netlify functions:serve",
    "start:app": "LOCAL_ENV=true NETLIFY_SERVER=http://localhost:9999 NODE_ENV=development webpack serve",
    "start": "yarn run start:netlify & yarn run start:app",
    "test": "jest"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "44.1.0",
    "@ckeditor/ckeditor5-react": "9.4.0",
    "@emotion/is-prop-valid": "1.3.1",
    "@floating-ui/react-dom": "2.1.2",
    "@fontsource/roboto": "5.1.1",
    "@netlify/blobs": "8.1.0",
    "@netlify/functions": "3.0.0",
    "@react-hive/honey-form": "2.5.0",
    "@react-hive/honey-layout": "3.6.0",
    "@tanstack/react-query": "4.36.1",
    "csstype": "3.1.3",
    "firebase-admin": "13.0.2",
    "jsonwebtoken": "9.0.2",
    "parse-multipart-data": "1.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.27.0",
    "react-toastify": "11.0.2",
    "stripe": "17.5.0",
    "styled-components": "6.1.14",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@eslint/js": "9.18.0",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "22.10.7",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "eslint": "9.18.0",
    "eslint-plugin-react": "7.37.4",
    "html-webpack-plugin": "5.6.3",
    "netlify-cli": "18.0.1",
    "prettier": "3.4.2",
    "sass": "1.83.4",
    "sass-loader": "16.0.4",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/src/**/__tests__/**/*.tsx"
    ]
  }
}
